#include <iostream>

using namespace std;

int main()
{
    long a,b,c,f,s,t;
    cin>>a>>b>>c;
    if(a<=b&&a<=c)
    {
        f=a;
        if(b<=c)
        {
         s=b;
         t=c;
        }
        else
        {
         s=c;
         t=b;
        }
    }
    else if(b<=a&&b<=c)
    {
        f=b;
        if(a<=c)
        {
         s=a;
         t=c;
        }
        else
        {
         s=c;
         t=a;
        }
    }    else if(c<=a&&c<=b)
    {
        f=c;
        if(a<=b)
        {
         s=a;
         t=b;
        }
        else
        {
         s=b;
         t=a;
        }
    }
    cout<<f<<"\n"<<s<<"\n"<<t<<"\n\n";
    cout<<a<<"\n"<<b<<"\n"<<c<<"\n\n";
    return 0;
}
